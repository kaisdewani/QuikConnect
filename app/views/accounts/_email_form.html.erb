 <%= form_tag("messages/email", method: "post") do %>
 <% @accounts.each do |account| %>
    <%= label_tag(:account_name, account.name) %>
    <%= check_box_tag 'account_urls[]', account.url %>
  <% end %>
  <%= label_tag(:string, "Email :") %>
  <%= email_field(:string, :email) %>
   <br>
  <%= submit_tag("Send") %>
<% end %> 