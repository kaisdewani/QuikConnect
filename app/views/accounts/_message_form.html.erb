<%= form_tag("messages/text", method: "post") do %>
  <% @accounts.each do |account| %>
    <%= label_tag(:account_name, account.name) %>
    <%= check_box_tag 'account_urls[]', account.url %>
  <% end %>
  <%= label_tag(:number, "Phone Number:") %>
  <%= telephone_field(:number, :phone) %>
   <br>
  <%= submit_tag("Send") %>
<% end %>