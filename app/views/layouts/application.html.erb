<!DOCTYPE html>
<html>
  <head>
    <title>TestQuickConnect</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
    

    
  </head>

  <body>
  <div class="container">
    <nav class="navbar navbar-default">
      <div class="container">
        <div class = "navbar-header">
          <%= link_to "Home", root_path, class: "navbar-brand"%>
          <%= link_to 'Accounts', accounts_path, class: "navbar-brand" %>
           <%= link_to 'Profile', profiles_path, class: "navbar-brand" %>
        </div>

        <ul class="nav navbar-nav">
        <% if user_signed_in? %>
        <li><%= link_to "Sign Out", destroy_user_session_path, method: :delete, class: "navbar-brand-one" %></li>
         <% elsif%>
          <li> <%= link_to "Log In", new_user_session_path, class: "navbar-brand-one" %> </li>  
            <% else%>
              <li> <%=link_to "Sign Up", new_user_registration_path, class: "navbar-brand-one"%> </li>
                  <% end %>
               
        </ul>
      </div>
    </nav>
  <div>

</div>
  
  <div >
    <% if notice %>
    <div class="alert alert-info" role="alert"><%= notice %></div>
    <% end %>
    <% if alert %>
    <div class="alert alert-danger" role="alert"><%= alert %></div>
    <% end %>

    <%= yield %>

  </div>
  </body>
</html>
