<%= simple_form_for @profile, url: profiles_path do |f| %>
  <%= f.input :first_name, label: "First Name", placeholder: "Your First Name" %>
  <%= f.input :last_name, label: "Last Name", placeholder: "Your Last Name" %>
  <%= f.input :personal_phone, label: "Phone Number", placeholder: "Business or Personal Phone Number"  %>
  <%= f.input :personal_address, label: "Address", placeholder: "Home or Business Address Here"  %>
  <%= f.button :submit %>
<% end %>

